<h1>All Albums</h1>
<ul>
    <% albums.forEach(album => { %>
        <li>
          <a href="albums/<%= album.id %>"> <%= album.name %> </a>
          <form
            action="albums/<%= album.id %>?_method=DELETE"
            method="POST"
          >
            <button type="submit" class="btn btn-danger btn-sm">Delete</button>
          </form>
        </li>
        <% }) %>
      </ul>